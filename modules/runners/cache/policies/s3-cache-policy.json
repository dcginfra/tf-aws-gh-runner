{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "githubActionCache",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:ListBucket",
        "s3:GetBucketLocation",
        "s3:ListBucketMultipartUploads",
        "s3:ListMultipartUploadParts"
      ],
      "Resource": [
        "${s3_arn}",
        "${s3_arn}/*"
      ]
    }
  ]
}
